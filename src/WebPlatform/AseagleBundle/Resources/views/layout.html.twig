{% block head %}
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title></title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="{{ asset('bundles/framework/vendor/bootstrap/css/bootstrap.min.css') }}">
    <style>
        body {
            padding-top: 50px;
            padding-bottom: 20px;
        }
    </style>
    <link rel="stylesheet" href="{{ asset('bundles/framework/vendor/bootstrap/css/offcanvas.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/framework/vendor/bootstrap/css/bootstrap-theme.min.css') }}">

    <link rel="stylesheet" href="{{ asset('bundles/framework/vendor/bootplus/css/bootplus.css') }}">
    <link rel="stylesheet" href="{{ asset('bundles/framework/vendor/yamm/yamm.css') }}">

    <script src="{{ asset('bundles/framework/vendor/bootstrap/js/modernizr-2.6.2-respond-1.1.0.min.js') }}"></script>


    <!--original css-->
    <link rel="icon" sizes="16x16" href="{{ asset('favicon.ico') }}" />
    <link rel="stylesheet" href="{{ asset('bundles/framework/css/style.css') }}" />
    <script src="{{ asset('bundles/framework/vendor/bootstrap/js/jquery-1.11.0.min.js') }}"></script>

{% endblock %}

{% block body %}
    <!--[if lt IE 7]>
    <p class="browsehappy">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->
    <!-- Navigation -->
    <div class="navbar yamm navbar-default navbar-fixed-top">
        <!--div class="container"-->
        <div class="navbar-header">
            <button type="button" data-toggle="collapse" data-target="#navbar-collapse-1" class="navbar-toggle collapsed">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button><a href="#" class="navbar-brand">Aseagle</a>
        </div>
        <div id="navbar-collapse-1" class="navbar-collapse collapse" style="height: 1px;">
            <ul class="nav navbar-nav">
                <!-- Grid 12 Menu -->
                <li class="dropdown yamm-fw"><a href="#" data-toggle="dropdown" class="dropdown-toggle">Menu<b class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li class="grid-demo">
                            <div class="row" id="aseagle-navbar" style="padding: 30px">
                                
                            </div>
                        </li>
                    </ul>
                </li>
            </ul>

            <div style="float: right">
                {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                    {{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }} |
                    <a href="{{ path('fos_user_security_logout') }}">
                        {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
                    </a>
                {% else %}
                    <!--<a href="{{ path('fos_user_security_login') }}">{{ 'layout.login'|trans({}, 'FOSUserBundle') }}</a> -->
                    <form action="{{ path("fos_user_security_check") }}" method="post">
                        <label>Email</label>
                        <input type="text" name="_username" required="required" />

                        <label>Password</label>
                        <input type="password" name="_password" required="required" />

                        <button type="submit">Submit</button>
                        |<a href="{{ path('fos_user_registration_register') }}">Register</a>
                        <input type="hidden" name="_csrf_token" value="{{ render(controller('AseagleBundle:Main:getToken'))}}" />
                    </form>
                {% endif %}
            </div>
        </div>

        <!--/div-->
    </div>



    <div class="block">
        {% block content %}{% endblock %}
    </div>

    <footer>
        <p>Â© Company 2014</p>
    </footer>


    <script src="{{ asset('bundles/framework/vendor/bootstrap/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('bundles/framework/vendor/bootstrap/js/offcanvas.js') }}"></script>
    <script src="{{ asset('bundles/framework/js/require.js') }}"></script>



    <script src="{{ asset('bundles/framework/js/_util/util.js') }}"></script>
    <script src="{{ asset('bundles/framework/js/_util/mapping_en.js') }}"></script>

    {% if code is defined %}
        <h2>Code behind this page</h2>
        <div class="block">
            <div class="symfony-content">{{ code|raw }}</div>
        </div>
    {% endif %}
{% endblock %}
